#include<iostream>
using namespace std;
int main()
{
	int i,j,n;
	cout << "Enter number of rows/column : ";
	cin >> n;
	i = 1;
	while(i <= n)
	{
		j = 1;
		while(j <= n)
		{
		cout << j;
		j++;	
		}
		i++;
		cout << endl;
	}
}


